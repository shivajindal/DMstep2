package com.stackroute.demo1;

public class StrBuff {

    void reverse(){
   	 
		 String input="london";      
        
        int length = input.length(); // string length
      for(int i=1;i<input.length()+1;i++) 
      {
          char a= input.charAt(length-i);//converting string into char backwards
        System.out.print(a); 
      }
    }
}