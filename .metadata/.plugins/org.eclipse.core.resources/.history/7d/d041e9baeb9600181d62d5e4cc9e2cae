package com.stackroute.pe2;

public class Even {
		public static boolean isEven(double num)
		{
		    if(num%2==0)  
		    {
		        return true;
		    }
		    else
		    {
		        return false;
		    }
		}
		     public static void main(String []args){
		 System.out.println(" result="+isEven(5));
		     }
		}