package com.stackroute.pe2;

public class Even {
		public static boolean Even(int num)
		{
		    if(num%2==0)  
		    {
		    	System.out.print(true);
		        return true;
		    }
		}
		 public static boolean Odd(int num)
		    {
			 if(num%2!=0) {
		    	System.out.print(false);
		        return false;
		    }
		}
}		   
		