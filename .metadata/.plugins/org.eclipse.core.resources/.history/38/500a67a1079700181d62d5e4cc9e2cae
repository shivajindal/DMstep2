package com.stackroute.pe2;

import java.util.*;
import java.io.*;
public class Factorial {
    public static long factorial(int num)
    {
         long fact=1;
        for(int i=1;i<=num;i++)
        {
            fact=fact*1;
        }
      return fact;
    }
    public static void intfactorial()
    {int a=1;
    while(a>0){
        long result=factorial(a);
if(result<2147483647)
{
 System.out.println("The factorial of "+a+" is "+result);   
}
else
{
    System.out.println("The factorial of "+a+" is Undefined");
    break;
}
a++;
}
    }
     public static void main (String []args) {
intfactorial();
}
}