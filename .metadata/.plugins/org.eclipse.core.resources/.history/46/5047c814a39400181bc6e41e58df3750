package com.stackroute.demo;
import java.util.*;


public class ConVow {
	
		public static void main(String[ ] arg)
		{
		int i=0;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter a word or character : ");
		String st = .next();	 
		
		
		for(i=0; i< st.length(); i++)
		if(st.charAt(i) =='a'||st.charAt(i)=='e'||st.charAt(i)=='i'||st.charAt(i)=='o'||st.charAt(i)=='u')
		{
		System.out.print("Entered character "+ st +" is  Vowel"); 
		}
		
		else if((st.charAt(i)>='a'&&st.charAt(i)<='z')||(st.charAt(i)>='A'&&st.charAt(i)<='Z'))
			System.out.print("Entered character "+st+" is Consonant");
		      else
			System.out.print("Not an alphabet");		
		}
	}
